{
	"name": "findbrok/laravel-personality-insights",
	"description": "Using IBM Watson Personality Insights Service with Laravel 5",
	"keywords": ["laravel", "ibm", "watson", "personality", "insights"],
	"license": "MIT",
	"authors": [
		{
			"name": "Percy Mamedy",
			"email": "percymamedy@gmail.com"
		}
	],
	"require": {
		"php": ">=5.5.0",
		"illuminate/support": "5.0 - 5.4",
		"findbrok/php-watson-api-bridge": "^1.0",
		"ramsey/uuid": "^3.4",
		"netresearch/jsonmapper": "^1.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0|~5.0",
		"mockery/mockery": "0.9.*",
		"orchestra/testbench": "~3.0"
	},
	"autoload": {
		"files": [
			"src/Support/helpers.php"
		],
		"psr-4": {
			"FindBrok\\PersonalityInsights\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.1.x-dev"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
